{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb/hooks"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        
            "no-use-before-define": ["error", {
                "functions": false,
                "classes": false,
                "variables": false,
                "allowNamedExports": false
            }]
        
    }
}
